{
  "name": "otrix-standalone-dashboard",
  "version": "0.0.1",
  "description": "The web-based dashboard of Cublick",
  "main": "./dist/main.js",
  "author": "Cublick Digital Co., Ltd. <support@cublick.com>",
  "license": "MIT",
  "maintainers": [
    "Seoung Min Baik <qortmdalsdl22@gmail.com>"
  ],
  "private": true,
  "build": {
    "appId": "com.cublick-standalone-dashboard",
    "productName": "Otrix_Standalone_Dashboard",
    "extraResources": "resources",
    "asar": true,
    "files": [
      "assets/",
      "dist/",
      "node_modules/",
      "package.json",
      "public/index.html",
      "public/locales/"
    ],
    "artifactName": "${productName}-setup-${version}.${ext}",
    "linux": {
      "target": "deb",
      "vendor": "ERCLab",
      "icon": "./resources/icons"
    },
    "mac": {
      "target": "default",
      "icon": "resources/icons/icon.icns"
    },
    "dmg": {
      "title": "Cublick Dashboard"
    },
    "win": {
      "target": "nsis",
      "icon": "resources/icons/icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "runAfterFinish": true,
      "deleteAppDataOnUninstall": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "installerIcon": "./resources/cublick.ico",
      "installerHeaderIcon": "./resources/cublick.png"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "scripts": {
    "package-linux32": "npm run build && build --linux --ia32",
    "package-linux64": "npm run build && build --linux --x64",
    "package-armv7": "npm run build && build --linux --armv7l",
    "package-arm64": "npm run build && build --linux --arm64",
    "package-macos": "npm run build && build --macos",
    "package-win64": "npm run build && build --win --x64",
    "package-win32": "npm run build && build --win --ia32",
    "build": "npm run build-electron && npm run build-renderer",
    "build-electron": "cross-env NODE_ENV=production webpack -r ts-node/register --config config/webpack.config.electron.ts --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production webpack -r ts-node/register --config config/webpack.config.production.ts --progress --profile --colors",
    "electron": "cross-env NODE_ENV=development electron -r ts-node/register src/electron.main.ts",
    "server": "cross-env NODE_ENV=development ts-node config/server.ts",
    "dev": "npm-run-all -p -r server electron",
    "rebuild": "./node_modules/.bin/electron-rebuild -f"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "bugs": {
    "url": "https://github.com/SSU-ERCLab/SDSS_WEB_DISPLAYER/issues"
  },
